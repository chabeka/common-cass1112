<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
   xsi:schemaLocation="
      http://www.springframework.org/schema/beans 
      http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
      http://www.springframework.org/schema/context 
      http://www.springframework.org/schema/context/spring-context-3.0.xsd">

   <import resource="applicationContext-service.xml" />
	<bean id="saeServiceProvider"
      class="fr.urssaf.image.sae.webservices.service.SAEServiceProviderImpl">
   </bean>

   <bean id="saeCaptureService" factory-bean="SAEServiceFactory"
      factory-method="createSAECaptureService" />
      
 	<bean id="saeBulkCaptureService" factory-bean="SAEServiceFactory"
      factory-method="createSAEBulkCaptureService" />
</beans>