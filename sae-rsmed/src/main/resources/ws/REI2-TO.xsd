			<xs:extension base="pivotReferentiel:EntrepriseResume_Type">
				<xs:sequence>
					<xs:element name="activitePrincipaleDeclaree"
						type="cfe:Activite420_Type" maxOccurs="1" minOccurs="0" />
					<xs:element name="urResponsable"
						type="datamodelPivot:CODE-ORGANISME" maxOccurs="1"
						minOccurs="1" />
					<xs:element name="dateCreation" type="xs:date"
						maxOccurs="1" minOccurs="0" />
					<xs:element name="dateDebutActivite" type="xs:date"
						maxOccurs="1" minOccurs="0" />
					<xs:element name="natureGerance"
						type="pivotReferentiel:NatureGerance_Type" maxOccurs="1" minOccurs="0">
					</xs:element>
					<xs:element name="nomCommercial" type="xs:string"
						minOccurs="0" />
					<xs:element name="dateCessation" type="xs:date"
						maxOccurs="1" minOccurs="0" />
					<xs:element name="dateDisparition" type="xs:date"
						maxOccurs="1" minOccurs="0" />
					<xs:element name="codeGreffeImmatRCS"
						type="pivotReferentiel:CodeGreffeImmatRCSCertification_Type"
						maxOccurs="1" minOccurs="0" />
					<xs:element name="codeDptImmatRM"
						type="pivotReferentiel:CodeDptImmatRMCertification_Type"
						maxOccurs="1" minOccurs="0" />
					<xs:element name="codeSigle"
						type="pivotReferentiel:CodeSigle_Type" maxOccurs="1"
						minOccurs="0" />
					<xs:element name="noRCS"
						type="pivotReferentiel:NoRCS_Type" maxOccurs="1" minOccurs="0" />
					<xs:element name="noRM"
						type="pivotReferentiel:NoRM_Type" maxOccurs="1" minOccurs="0" />
					<xs:element name="idAgessa"
						type="pivotReferentiel:NoAGESSA_Type" maxOccurs="1" minOccurs="0" />
					<xs:element name="idMdArtiste"
						type="pivotReferentiel:NoMDArtiste_Type" maxOccurs="1"
						minOccurs="0" />
