	<xs:complexType name="EtablissementComplet_Type">
		<xs:annotation><xs:documentation>Etablissement avec ses objects connexes (son entreprise, l'adresse, et l'ensemble des redevabilites)</xs:documentation></xs:annotation>
		<xs:complexContent>
			<xs:extension base="pivotReferentiel:Etablissement_Type">
				<xs:sequence>
					<xs:element name="entreprise" type="pivotReferentiel:Entreprise_Type" minOccurs="0" maxOccurs="1" />
					<xs:element name="adresse" type="pivotReferentiel:Adresse_Type" minOccurs="0" maxOccurs="1" />
					<xs:element name="redevabilites" type="pivotReferentiel:Redevabilite_Type" minOccurs="0" maxOccurs="unbounded" />
					<xs:element name="lienCompte" type="pivotReferentiel:LienCompte_Type" minOccurs="0" maxOccurs="1" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:complexType name="Etablissement_Type">
		<xs:annotation><xs:documentation>L'etablissement est la plus petite unite de production, localisee geographiquement, individualisee. Il depend juridiquement d'une entreprise. Sa production est relativement homogene. Le siege est l'etablissement principal, lorsqu'une entreprise n'exerce pas ses activites dans un seul etablissement.</xs:documentation></xs:annotation>
		<xs:complexContent>
			<xs:extension base="pivotReferentiel:EtablissementResume_Type">
				<xs:sequence>
					<xs:element name="codeNaf"
						type="pivotReferentiel:CodeNAFCertification_Type" minOccurs="0" />
					<xs:element name="dateCreation" type="xs:dateTime"
						minOccurs="0" />
					<xs:element name="dateDebutActivite" type="xs:dateTime"
						maxOccurs="1" minOccurs="0"></xs:element>
					<xs:element name="dateFinActivite" type="xs:dateTime"
						minOccurs="0" />
					<xs:element name="dateMiseEnLocGerance" type="xs:dateTime"
						minOccurs="0" />					<xs:element name="presencePersonnel" type="xs:boolean"
						minOccurs="0" />					<xs:element name="activitePlusImportante"
						type="pivotReferentiel:ActivitePlusImportante_Type" maxOccurs="1"
						minOccurs="0" />
					<xs:element name="ensActivitesExercees"
						type="pivotReferentiel:EnsActivitesExercees_Type" maxOccurs="1"
						minOccurs="0" />
					<xs:element name="etablissementSuccesseur"
						type="pivotReferentiel:IdREI_Type" maxOccurs="1"
						minOccurs="0" />
					<xs:element name="etablissementPredecesseur"
						type="pivotReferentiel:IdREI_Type" maxOccurs="1"
						minOccurs="0" />
					<xs:element name="dateDebutEmbauche" type="xs:dateTime"
						minOccurs="0" />
					<xs:element name="dateFinEmbauche" type="xs:dateTime"
						minOccurs="0" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="EtablissementResume_Type">
		<xs:annotation><xs:documentation>EtablissementResume contient une sous partie des informations propres a un etablissement</xs:documentation></xs:annotation>
		<xs:complexContent>
			<xs:extension base="pivotReferentiel:EntiteCotisante_Type">
				<xs:sequence>
					<xs:element name="urGestionnaire"
						type="datamodelPivot:CODE-ORGANISME" maxOccurs="1"
						minOccurs="0" />
					<xs:element name="siret" type="pivotReferentiel:SIRETCertification_Type"
						minOccurs="0" maxOccurs="1"/>
					<xs:element name="enseigne" type="xs:string" minOccurs="0" />
					<xs:element name="categorieEtablissement" type="pivotReferentiel:CategorieEtablissement_Type"
						maxOccurs="1" minOccurs="0"></xs:element>
					<xs:element name="dateFermeture" type="xs:dateTime"
						minOccurs="0" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="EntiteCotisante_Type" abstract="true">
		<xs:annotation><xs:documentation>Une entite cotisante peut etre soit un individu, une entreprise ou un etablissement.</xs:documentation></xs:annotation>
		<xs:sequence>
			<xs:element name="id" type="pivotReferentiel:IdREI_Type"
				minOccurs="1" maxOccurs="1" />
			<xs:element name="typeEntite" type="xs:int" minOccurs="0" />
			<xs:element name="codeOrigine" type="xs:int" maxOccurs="1"
				minOccurs="0"></xs:element>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="EntrepriseComplete_Type">
		<xs:annotation><xs:documentation>Entreprise avec ses objets connexes (adresse, ensemble des etablissements, des redevabilites, des dirigeants)</xs:documentation></xs:annotation>
		<xs:complexContent>
			<xs:extension base="pivotReferentiel:Entreprise_Type">
				<xs:sequence>
					<xs:element name="adresseSiege" type="pivotReferentiel:Adresse_Type" minOccurs="0" maxOccurs="1" />
					<xs:element name="etablissements" type="pivotReferentiel:EtablissementComplet_Type" minOccurs="0" maxOccurs="unbounded" />
					<xs:element name="redevabilites" type="pivotReferentiel:Redevabilite_Type" minOccurs="0" maxOccurs="unbounded" />
					<xs:element name="dirigeants" type="pivotReferentiel:DirigeantPersPhysique_Type" minOccurs="0" maxOccurs="unbounded" />
					<xs:element name="nbEtablissements" type="xs:int" minOccurs="0" maxOccurs="1" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="Entreprise_Type">
