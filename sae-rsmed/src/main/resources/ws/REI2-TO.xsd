			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="Individu_Type">
		<xs:annotation><xs:documentation>Un individu est une personne physique redevable ou potentiellement redevable de cotisations sociales a quelque titre que ce soit professionnellement ou personnellement	</xs:documentation></xs:annotation>
		<xs:complexContent>
			<xs:extension base="pivotReferentiel:IndividuResume_Type">
				<xs:sequence>
					<xs:element name="pseudonyme" type="cfe:Prenom_Type"
						maxOccurs="1" minOccurs="0" />
					<xs:element name="cleNir"
						type="pivotReferentiel:NIRCleCertification_Type" maxOccurs="1"
						minOccurs="0" />
					<xs:element name="validiteNir"
						type="xs:string" maxOccurs="1"
						minOccurs="0" />
					<xs:element name="certificatNirRsi"
						type="xs:string" maxOccurs="1"
						minOccurs="0" />
					<xs:element name="idAgessa"
						type="pivotReferentiel:NoAGESSA_Type" maxOccurs="1" minOccurs="0" />
					<xs:element name="idMdArtiste"
						type="pivotReferentiel:NoMDArtiste_Type" maxOccurs="1"
						minOccurs="0" />
					<xs:element name="pseudoSiret" type="pivotReferentiel:SIRETAlphanumerique_Type"
						maxOccurs="1" minOccurs="0" />
					<xs:element name="sirenPersonnel"
						type="pivotReferentiel:SIRENAlphanumeriqueCertification_Type" maxOccurs="1"
						minOccurs="0" />
					<xs:element name="noCpam" type="xs:string"
						maxOccurs="1" minOccurs="0" />
					<xs:element name="libelleCommuneNaissance"
						type="pivotReferentiel:CommuneCertification_Type" maxOccurs="1"
						minOccurs="0" />
					<xs:element name="codeSituationFamille"
						type="pivotReferentiel:CodeSituationMatrimoniale_Type" maxOccurs="1" minOccurs="0" />
					<xs:element name="codeNationalite"
						type="pivotReferentiel:CodeNationaliteCertification_Type"
						maxOccurs="1" minOccurs="0" />
					<xs:element name="nationalite"
						type="cfe:Nationalite_Type" maxOccurs="1" minOccurs="0" />
					<xs:element name="dateDeces"
						type="pivotReferentiel:DateCertification_Type" maxOccurs="1"
						minOccurs="0" />
					<xs:element name="certificatDeces"
						type="pivotReferentiel:CertificatDecesCertification_Type"
						maxOccurs="1" minOccurs="0" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:simpleType name="CodeSituationMatrimoniale_Type">
		<xs:annotation>
			<xs:documentation>
			Code matrimonial :
				01	Célibataire
				02	Marié
				03	Divorcé
				04	Séparé
				05	Veuf/Veuve
				06	Vie maritale
				07	PACS
				90	Non connue  
			</xs:documentation>			
		</xs:annotation>
		<xs:restriction base="cfe:AlphaNum_Type">
			<xs:length value="2" />
		</xs:restriction>
	</xs:simpleType>
	
	<xs:complexType name="IndividuResume_Type">
		<xs:annotation><xs:documentation>IndividuResume contient une sous partie des informations propres a un individu</xs:documentation></xs:annotation>
		<xs:complexContent>
			<xs:extension base="pivotReferentiel:EntiteCotisante_Type">
				<xs:sequence>
					<xs:element name="nir" type="pivotReferentiel:NIRNumeroCertification_Type" maxOccurs="1" minOccurs="0" />
					<xs:element name="civilite" type="pivotReferentiel:Civilite_Type" maxOccurs="1" minOccurs="0" />
					<xs:element name="nomPatronymique" type="pivotReferentiel:NomCertification_Type" maxOccurs="1" minOccurs="0" />
					<xs:element name="nomMarital" type="cfe:Nom_Type" maxOccurs="1" minOccurs="0" />
					<xs:element name="nomUsage" type="cfe:Nom_Type" maxOccurs="1" minOccurs="0" />
					<xs:element name="prenomsPatronymiques" type="pivotReferentiel:PrenomsPatronymiquesCertification_Type" maxOccurs="1" minOccurs="0" />
					<xs:element name="prenomUsuel" type="pivotReferentiel:PrenomCertification_Type" maxOccurs="1" minOccurs="0"></xs:element>
					<xs:element name="dateNaissance" type="pivotReferentiel:DateCertification_Type" maxOccurs="1" minOccurs="0" />
					<xs:element name="codeCommuneInseeNaissance" type="pivotReferentiel:CodeGeoINSEECertification_Type" maxOccurs="1" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								Pour la France uniquement
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="codePaysNaissance" type="pivotReferentiel:CodeGeoINSEECertification_Type" maxOccurs="1" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								Si hors France
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="noRiba" type="pivotReferentiel:NoRIBACertification_Type" maxOccurs="1" minOccurs="0" />
					<xs:element name="numeroMarin" type="xs:string" maxOccurs="1" minOccurs="0" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

