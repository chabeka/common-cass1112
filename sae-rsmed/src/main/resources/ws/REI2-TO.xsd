	<xs:complexType name="EntrepriseResume_Type">
		<xs:annotation><xs:documentation>EntrepriseResume contient une sous partie des informations propres a une entreprise</xs:documentation></xs:annotation>
		<xs:complexContent>
			<xs:extension base="pivotReferentiel:EntiteCotisante_Type">
				<xs:sequence>
					<xs:element name="siren" type="pivotReferentiel:SIRENCertification_Type" maxOccurs="1" minOccurs="0" />
					<xs:element name="denomination" type="cfe:Denomination_Type" maxOccurs="1" minOccurs="0" />
					<xs:element name="formeJuridiqueINSEE" type="pivotReferentiel:CodeCJCertification_Type" maxOccurs="1" minOccurs="0" />
					<xs:element name="codeNAF" type="pivotReferentiel:CodeNAFCertification_Type" maxOccurs="1" minOccurs="0" />
                    <xs:element name="type" type="pivotReferentiel:TypeEntreprise_Type" maxOccurs="1" minOccurs="1" />
