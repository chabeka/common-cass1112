					<xs:element name="adresseDomicile"
						type="pivotReferentiel:Adresse_Type" minOccurs="0" maxOccurs="1" />
					<xs:element name="redevabilites"
						type="pivotReferentiel:Redevabilite_Type" minOccurs="0"
						maxOccurs="unbounded" />
					<xs:element name="collaborateurs"
						type="pivotReferentiel:CollaborateurComplet_Type" minOccurs="0"
						maxOccurs="unbounded" />
					<xs:element name="dirigeants"
						type="pivotReferentiel:CollaborateurComplet_Type" minOccurs="0"
						maxOccurs="unbounded" />
