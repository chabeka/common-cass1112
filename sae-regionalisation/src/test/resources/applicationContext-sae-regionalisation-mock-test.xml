<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
   xsi:schemaLocation="
		http://www.springframework.org/schema/beans	
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context 
        http://www.springframework.org/schema/context/spring-context-3.0.xsd">

   <bean id="applicationProperties"
      class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
      <property name="location"
         value="classpath:config/sae-config-test.properties" />
   </bean>

   <bean id="saeDocumentDaoImpl" factory-bean="daoMock"
      factory-method="createSaeDocumentDao" />

   <bean id="serviceProviderSupportImpl" factory-bean="daoMock"
      factory-method="createServiceProviderSupport" />

   <bean id="searchCriterionDaoImpl" factory-bean="daoMock"
      factory-method="createSearchCriterionDao" />

   <bean id="metadataDaoImpl" factory-bean="daoMock" factory-method="createMetadataDao" />

   <bean id="traceDaoImpl" factory-bean="daoMock" factory-method="createTraceDao" />

   <import resource="applicationContext-sae-regionalisation-service.xml" />
   
</beans>