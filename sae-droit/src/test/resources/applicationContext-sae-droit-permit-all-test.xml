<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
   xsi:schemaLocation="
		http://www.springframework.org/schema/beans	
		http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context 
        http://www.springframework.org/schema/context/spring-context.xsd">

   <bean
      class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
      <property name="locations">
         <list>
            <value>config/sae-config.properties</value>
            <value>classpath:config/cassandra-local.properties</value>
         </list>
      </property>
   </bean>

   <import resource="applicationContext-sae-droit-permit-all.xml" />

   <bean id="prmdControleTest"
      class="fr.urssaf.image.sae.droit.controle.PrmdControleTestImpl" />

   <bean class="fr.urssaf.image.sae.droit.cache.CacheConfig" id="cacheConfig" />

   <!--
      Importation de la dÃ©finition du keyspace cassandra et du client
      zookeeper (zkClient)
   -->
   <import resource="classpath*:/applicationContext-commons-cassandra.xml" />
   
</beans>