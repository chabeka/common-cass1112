<configuration>

   <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
      <encoder>
         <pattern>%d %p %m%n</pattern>
         <charset>UTF-8</charset>
      </encoder>
   </appender>

   
   <logger name="fr.urssaf.image.sae">
      <level value="INFO" />
   </logger>


   <!-- Niveau general pour l'application -->
   <root level="WARN">
      <appender-ref ref="console" />
   </root>


</configuration>