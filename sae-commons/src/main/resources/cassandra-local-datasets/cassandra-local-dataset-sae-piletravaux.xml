<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<keyspace xmlns="http://xml.dataset.cassandraunit.org"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://xml.dataset.cassandraunit.org cassandraUnitDataset.xsd ">
   
   <name>KEYSPACE_TU</name>
   
   <strategy>org.apache.cassandra.locator.SimpleStrategy</strategy>
   <replicationFactor>1</replicationFactor>
   
   <columnFamilies>
      
      <!--
      La pile des travaux
      -->
      <columnFamily>
         <name>JobRequest</name>
         <comparatorType>BytesType</comparatorType>
      </columnFamily>
      <columnFamily>
         <name>JobsQueue</name>
         <comparatorType>TimeUUIDType</comparatorType>
      </columnFamily>
      <columnFamily>
         <name>JobHistory</name>
         <comparatorType>TimeUUIDType</comparatorType>
      </columnFamily>
      
   </columnFamilies>
   
</keyspace>