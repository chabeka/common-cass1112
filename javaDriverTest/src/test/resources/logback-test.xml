<configuration>

   <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
      <encoder>
         <!-- <pattern>%d{HH:mm:ss.SSS} [%-5level] %thread %logger%n%msg%n%n</pattern>-->
         <pattern>%msg%n</pattern>
      </encoder>
   </appender>

   <logger name="fr.urssaf.javaDriverTest.split.NewSplitsScheduler" level="DEBUG" />

   <root level="WARN">
      <appender-ref ref="STDOUT" />
   </root>

</configuration>