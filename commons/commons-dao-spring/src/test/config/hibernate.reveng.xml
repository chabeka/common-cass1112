<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="auteur">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="document">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="etat">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>

	<table name="document" class="fr.urssaf.image.commons.dao.spring.modele.Document">
		<column name="id"></column>
		<column name="titre" property="titre"></column>
		<column name="date"></column>
		<column name="id_auteur"></column>
	</table>
<table name="document_view">
	<primary-key>
		<key-column name="id" property="id" />
	</primary-key>
	<column name="titre"></column>
	<column name="date"></column>
</table>
</hibernate-reverse-engineering>