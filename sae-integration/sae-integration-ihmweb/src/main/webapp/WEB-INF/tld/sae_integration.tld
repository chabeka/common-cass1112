      <path>/WEB-INF/tags/comptagesTdm.tag</path>
   </tag-file>

   <tag-file>
      <name>vi</name>
      <description>Paramètres du VI</description>
      <path>/WEB-INF/tags/vi.tag</path>
   </tag-file>

   <tag-file>
      <name>lienRegistreSurvTec</name>
      <description>Ajout d'un lien vers le registre de surveillance
