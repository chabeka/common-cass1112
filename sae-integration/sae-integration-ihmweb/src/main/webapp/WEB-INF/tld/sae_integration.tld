      <path>/WEB-INF/tags/lienRegistreSurvTec.tag</path>
   </tag-file>
   
   <tag-file>
      <name>ajoutNote</name>
      <description>Ajout d'une note à un document</description>
      <path>/WEB-INF/tags/ajoutnote.tag</path>
   </tag-file>
   
   <tag-file>
      <name>stockageUnitaire</name>
      <description>Appel du service web de capture unitaire</description>
      <path>/WEB-INF/tags/stockageUnitaire.tag</path>
   </tag-file>

  

