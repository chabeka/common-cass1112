<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
   xsi:schemaLocation="
      http://www.springframework.org/schema/beans  
      http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
      http://www.springframework.org/schema/context 
      http://www.springframework.org/schema/context/spring-context-3.0.xsd">

   <!-- sae-config.properties -->
   <bean id="saeConfigResource" class="org.springframework.core.io.FileSystemResource">
      <constructor-arg
         value="src/test/resources/config/test-sae-config.properties" />
   </bean>

   <import resource="applicationContext-sae-lotinstallmaj.xml" />

   <bean id="SAECassandraDao" class="fr.urssaf.image.sae.lotinstallmaj.dao.SAECassandraDao">
      <property name="config" ref="configCassandra"/>
   </bean>
   <bean id="SAECassandraService"
      class="fr.urssaf.image.sae.lotinstallmaj.service.impl.SAECassandraService">
      <property name="saeDao" ref="SAECassandraDao" />
   </bean>


</beans>