<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
   xsi:schemaLocation="
		http://www.springframework.org/schema/beans	
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context 
        http://www.springframework.org/schema/context/spring-context-3.0.xsd">

   <import resource="applicationContext-sae-commons.xml" />

   <!-- Chargement des fichiers properties -->
   <bean class="fr.urssaf.image.sae.commons.factory.PropertiesFactory"
      factory-method="load">
      <constructor-arg ref="saeConfigResource" />
   </bean>

   <!-- Le PropertyPlaceholderConfigurer -->
   <bean id="applicationProperties" class="fr.urssaf.image.sae.commons.factory.PropertiesFactory"
      factory-method="load">
      <constructor-arg ref="saeConfigResource" />
   </bean>


   <!-- Importation contexte RND  -->
   <import resource="applicationContext-sae-rnd.xml" />


</beans>