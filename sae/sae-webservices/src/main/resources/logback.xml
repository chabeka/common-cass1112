<configuration>

   <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
      <encoder>
         <pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} %-5level [%logger] - %msg%n</pattern>
      </encoder>
   </appender>

   <logger name="fr.urssaf.image.sae">
      <level value="DEBUG" />
   </logger>

   <root level="WARN">
      <appender-ref ref="STDOUT" />
   </root>

</configuration>