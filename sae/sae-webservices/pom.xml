	<artifactId>sae-webservices</artifactId>
	<name>sae-webservices</name>
	<description>Services web du SAE</description>
	<packaging>war</packaging>

	<properties>
		<axiom.version>1.2.22</axiom.version>
		<sonar.exclusions>fr/cirtil/www/saeservice/*.java,fr/urssaf/image/sae/webservices/skeleton/SaeServiceMessageReceiverInOut.java,fr/urssaf/image/sae/webservices/client/modele/*</sonar.exclusions>
	</properties>

	<profiles>
		<profile>
			<id>generateSkeleton</id>
			<build>
				<plugins>
					<plugin>
						<groupId>org.apache.axis2</groupId>
						<artifactId>axis2-wsdl2code-maven-plugin</artifactId>
						<version>${axis2.version}</version>
						<executions>
							<execution>
								<goals>
									<goal>wsdl2code</goal>
								</goals>
								<configuration>
									<packageName>fr.urssaf.image.sae.webservices.skeleton</packageName>
									<wsdlFile>src/test/resources/SaeService.wsdl</wsdlFile>
									<wsdlVersion>1.1</wsdlVersion>
									<outputDirectory>src</outputDirectory>
									<generateServicesXml>true</generateServicesXml>
									<generateServerSide>true</generateServerSide>
									<skipBuildXML>true</skipBuildXML>
									<skipWSDL>false</skipWSDL>
									<generateServerSideInterface>true</generateServerSideInterface>
									<targetSourceFolderLocation>main/java</targetSourceFolderLocation>
									<targetResourcesFolderLocation>main/webapp/WEB-INF/services/SaeService/META-INF</targetResourcesFolderLocation>
								</configuration>
							</execution>
						</executions>
					</plugin>

					<plugin>
						<groupId>com.google.code.maven-replacer-plugin</groupId>
						<artifactId>maven-replacer-plugin</artifactId>
						<version>${maven-replacer-plugin.version}</version>
						<executions>
							<execution>
								<phase>generate-sources</phase>
								<goals>
									<goal>replace</goal>
								</goals>
							</execution>
						</executions>
						<configuration>
							<basedir>src/main/java</basedir>
							<includes>
								<include>**/cirtil/www/saeservice/*.java</include>
							</includes>
							<replacements>
								<replacement>
									<token>[ ]*@SuppressWarnings\(\{"unchecked","unused"\}\)</token>
									<valueFile>src/main/config/add_suppress_warnings.txt</valueFile>
								</replacement>
							</replacements>
						</configuration>
					</plugin>

				</plugins>
			</build>
		</profile>

		<profile>
			<id>genererCodeClientSaeService</id>
			<build>
				<plugins>
					<plugin>
						<groupId>org.apache.axis2</groupId>
						<artifactId>axis2-wsdl2code-maven-plugin</artifactId>
						<version>${axis2.version}</version>
						<executions>
							<execution>
								<goals>
									<goal>wsdl2code</goal>
								</goals>
								<configuration>
									<wsdlFile>src/test/resources/SaeService.wsdl</wsdlFile>
									<packageName>fr.urssaf.image.sae.webservices.client.modele</packageName>
									<wsdlVersion>1.1</wsdlVersion>
									<outputDirectory>src</outputDirectory>
									<databindingName>adb</databindingName>
									<targetSourceFolderLocation>main/java</targetSourceFolderLocation>
									<skipBuildXML>true</skipBuildXML>
									<overWrite>true</overWrite>
								</configuration>
							</execution>
						</executions>
					</plugin>

					<plugin>
						<groupId>com.google.code.maven-replacer-plugin</groupId>
						<artifactId>maven-replacer-plugin</artifactId>
						<version>1.3.6</version>

						<executions>
							<execution>
								<phase>generate-sources</phase>
								<goals>
									<goal>replace</goal>
								</goals>
							</execution>
						</executions>
						<configuration>
							<basedir>src/main/java</basedir>
							<includes>
								<include>fr/urssaf/image/sae/webservices/client/modele/*.java</include>
							</includes>
							<replacements>
								<replacement>
									<token>[ ]*@SuppressWarnings\(\{"unchecked","unused"\}\)</token>
									<valueFile>src/main/config/add_suppress_warnings.txt</valueFile>
								</replacement>
							</replacements>
						</configuration>
					</plugin>

				</plugins>
			</build>
		</profile>
		<profile>
			<id>hawai_livraison</id>
			<activation>
				<activeByDefault>false</activeByDefault>
			</activation>

			<build>
				<plugins>
					<!-- PLUGIN HAWAI/FEDEX -->
					<plugin>
						<groupId>fr.acoss.industrialisation.maven</groupId>
						<artifactId>hawai-maven-plugin</artifactId>
						<configuration>
							<applicationName>ged</applicationName>
							<applicationBinFileName>${project.build.finalName}.war</applicationBinFileName>
							<fedex>false</fedex>
							<skip>false</skip>
						</configuration>
					</plugin>
				</plugins>
			</build>
		</profile>
	</profiles>

	<build>
		<finalName>${project.name}</finalName>
		<plugins>
         <!-- COMPILATION ASPECT -->
			<plugin>
				<inherited>true</inherited>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>aspectj-maven-plugin</artifactId>
				<configuration>
					<argumentFileName>builddef-sae-webservices.lst</argumentFileName>
				</configuration>
				<dependencies>
