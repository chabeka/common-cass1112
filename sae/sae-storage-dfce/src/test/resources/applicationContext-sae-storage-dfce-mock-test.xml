<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
   xsi:schemaLocation="
		http://www.springframework.org/schema/beans	
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/context 
		http://www.springframework.org/schema/context/spring-context-3.0.xsd">

   <import resource="applicationContext-properties-test.xml"/>
   <import resource="applicationContext-commons-dfce-connection.xml" />
   <import resource="applicationContext-commons-cassandra.xml" />
   <import resource="applicationContext-sae-commons.xml" />
   <import resource="applicationContext-sae-droit.xml" />
   <import resource="applicationContext-sae-storage-dfce.xml" />

   <bean id="traceAssertUtils"
      class="fr.urssaf.image.sae.storage.dfce.utils.TraceAssertUtils" />

   <bean id="mockFactory"
      class="fr.urssaf.image.sae.storage.dfce.utils.MockFactoryBean" />

   <bean id="base" factory-bean="mockFactory" factory-method="createBase"
      primary="true">
   </bean>

   <bean id="dfceServices" factory-bean="mockFactory" factory-method="createDFCESercices"
      primary="true">
   </bean>

</beans>