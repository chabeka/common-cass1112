<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<droit xmlns="http://www.cirtil.fr/saeIntegration/droit">

   <listePrmd>
   
      <!-- PRMD pour les mandats SEPA de CTC -->
      <prmd>
         <code>PRMD_CTC_MANDATS_SEPA</code>
         <description>CTC - Mandats SEPA</description>
         <!--
            La metadonnee FormatFichier n'est pas presente dans le filtrage de la requete, car :
               - elle n'est fonctionnellement utile que lors de l'archivage
               - techniquement, elle ne peut pas etre utilisee comme critere de recherche (referentiel des metadonnees 1.6)
         -->
         <lucene>CodeRND:1.2.2.4.3 AND ApplicationProductrice:CTC AND ApplicationTraitement:CTC</lucene>
         <metadonnees>
            <metadonnee>
               <code>CodeRND</code>
               <valeurs>
                  <valeur>1.2.2.4.3</valeur>
               </valeurs>
            </metadonnee>
            <metadonnee>
               <code>ApplicationProductrice</code>
               <valeurs>
                  <valeur>CTC</valeur>
               </valeurs>
            </metadonnee>
            <metadonnee>
               <code>ApplicationTraitement</code>
               <valeurs>
                  <valeur>CTC</valeur>
               </valeurs>
            </metadonnee>
            <metadonnee>
               <code>FormatFichier</code>
               <valeurs>
                  <valeur>fmt/354</valeur>
               </valeurs>
            </metadonnee>
         </metadonnees>
         <bean />
      </prmd>
   </listePrmd>
   
   
   <listeCs>
   
      <!-- Contrat de service d'accrochage pour CTC Mandats SEPA -->
      <cs>
         <issuer>CS_CTC_ACCROCHAGE</issuer>
         <code>CS_CTC_ACCROCHAGE</code>
         <description>Contrat de service pour le client CTC</description>
         <viduree>7200</viduree>
         <pagms>

            <pagm>
               <code>CTC_SEPA_ARCHIVAGE_UNITAIRE</code>
               <description>CTC - Archivage unitaire des mandats SEPA</description>
               <pagma>
                  <code>ACCROCHAGE_CTC_SEPA_ARCHIVAGE_UNITAIRE_PAGMa</code>
                  <actions>
                     <action>archivage_unitaire</action>
                  </actions>
               </pagma>
               <pagmp>
                  <code>ACCROCHAGE_CTC_SEPA_ARCHIVAGE_UNITAIRE_PAGMp</code>
                  <description>Mandats SEPA</description>
                  <prmd>PRMD_CTC_MANDATS_SEPA</prmd>
               </pagmp>
               <parametres />
            </pagm>

            <pagm>
               <code>CTC_SEPA_RECHERCHE_CONSULTATION</code>
               <description>CTC - Recherche et consultation des mandats SEPA</description>
               <pagma>
                  <code>ACCROCHAGE_CTC_SEPA_RECHERCHE_CONSULTATION_PAGMa</code>
                  <actions>
                     <action>recherche</action>
                     <action>consultation</action>
                  </actions>
               </pagma>
               <pagmp>
                  <code>ACCROCHAGE_CTC_SEPA_RECHERCHE_CONSULTATION_PAGMp</code>
                  <description>Mandats SEPA</description>
                  <prmd>PRMD_CTC_MANDATS_SEPA</prmd>
               </pagmp>
               <parametres />
            </pagm>
         </pagms>
         <cnPki>CN=IGC/A</cnPki>
         <verifCnCert>false</verifCnCert>
         <cnCert />
         
      </cs>
   
   </listeCs>
   
   
</droit>