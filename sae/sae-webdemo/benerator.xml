<?xml version="1.0" encoding="utf-8"?>
<setup xmlns="http://databene.org/benerator/0.6.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://databene.org/benerator/0.6.0 http://databene.org/benerator-0.6.0.xsd">

   <database id="db" url="{dbUrl}" driver="{dbDriver}" schema="{dbSchema}"
      user="{dbUser}" password="{dbPassword}" />

   <echo message="initialisation de la base" />

   <execute uri="src/main/resources/webdemo-schema.sql" target="db"
      onError="ignore" />

   <echo message="load des donnÃ©es" />

   <generate type="logger" count="1000" consumer="db">

      <id name="idseq" type="int"
         generator="new
		org.databene.benerator.primitive.IncrementalIdGenerator(1)" />
      <attribute name="occurences" type="short" />
      <attribute name="problemes" type="string" />
      <attribute name="action" type="string" />
      <attribute name="infos" type="string" />
      <attribute name="horodatage" type="timestamp"
         generator="new org.databene.benerator.primitive.datetime.DateTimeGenerator()"
         min="2010-01-01 00:00:00" max="2010-12-31 23:59:59" />

   </generate>

</setup>

