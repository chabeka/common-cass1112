<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
   xsi:schemaLocation="
		http://www.springframework.org/schema/beans	
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context 
        http://www.springframework.org/schema/context/spring-context-3.0.xsd">

   <bean id="permitAll"
      class="fr.urssaf.image.sae.droit.controle.impl.skip.PrmdControlePermitAll" />
      
   <bean id="permitDomaineTechnique"
      class="fr.urssaf.image.sae.droit.controle.impl.skip.PrmdControleDomaineTechnique" />

   <bean class="fr.urssaf.image.sae.droit.cache.CacheConfig" id="cacheConfig">
      <property name="droitsCacheDuration" value="${sae.droits.cache}" />
   </bean>
</beans>