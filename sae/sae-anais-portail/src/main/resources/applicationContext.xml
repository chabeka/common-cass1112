<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="
		http://www.springframework.org/schema/beans	
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

   <bean id="configuration"
      class="fr.urssaf.image.sae.anais.portail.configuration.AnaisConfiguration">
      <property name="environnement" value="Production" />
      <property name="compteApplicatif" value="Sae" />
   </bean>

   <bean id="configurationSuccess"
      class="fr.urssaf.image.sae.anais.portail.configuration.SuccessConfiguration">
      <property name="url" value="succes.html" />
      <property name="service" value="/serviceTest" />
   </bean>

   <bean id="messageSource"
      class="org.springframework.context.support.ReloadableResourceBundleMessageSource">
      <property name="basenames">
         <list>
            <value>/message/labels</value>
         </list>
      </property>
      <property name="cacheSeconds" value="0" />
   </bean>

</beans>