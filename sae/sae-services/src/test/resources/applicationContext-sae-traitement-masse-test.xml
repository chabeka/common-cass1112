<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
   xsi:schemaLocation="
		http://www.springframework.org/schema/beans	
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context 
        http://www.springframework.org/schema/context/spring-context-3.0.xsd">

   <import resource="applicationContext-sae-traitement-masse.xml" />

   <bean
      class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
      <property name="locations">
         <value>classpath:config/commons-cassandra.properties</value>
      </property>
   </bean>

   <!--
      creation d'un job de test pour la capture en masse en attendant de
      pouvoir faire un mock du service de capture en masse
   -->
   <import resource="applicationContext-sae-batch-test.xml" />
   <import resource="jobs/job-capture-masse-test.xml" />


</beans>
