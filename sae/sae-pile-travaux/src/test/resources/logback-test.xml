<configuration>

   <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
      <encoder>
         <pattern>%d{yyyy-MM-dd HH:mm:ss.SSSk} %-5level %logger - %msg%n</pattern>
      </encoder>
   </appender>

   <logger name="fr.urssaf.image.sae">
      <level value="DEBUG"/>
   </logger>

   <!-- Niveau general pour l'application -->
   <root level="WARN">
      <appender-ref ref="console" />
   </root>

</configuration>