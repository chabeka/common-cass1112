<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
   xsi:schemaLocation="
		http://www.springframework.org/schema/beans	
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context 
        http://www.springframework.org/schema/context/spring-context-3.0.xsd">

   <context:component-scan base-package="fr.urssaf.image.sae.ordonnanceur" />

   <!-- importation du contexte de commons cassandra spring batch  -->
   <import resource="applicationContext-commons-cassandra-spring-batch.xml" />

   <!-- traitement de capture en masse -->
   <bean id="captureMasseLauncher"
      class="fr.urssaf.image.sae.ordonnanceur.support.impl.CaptureMasseLauncherSupportImpl">
      <qualifier value="captureMasseLauncher" />
      <constructor-arg value="${sae.archivagemasse.executable}" />
      <constructor-arg ref="saeConfigResource" />
   </bean>

</beans>