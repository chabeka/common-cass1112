<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
   xsi:schemaLocation="
		http://www.springframework.org/schema/beans	
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context 
        http://www.springframework.org/schema/context/spring-context-3.0.xsd">

   <context:component-scan base-package="fr.urssaf.image.sae.ordonnanceur.service" />
   <context:component-scan base-package="fr.urssaf.image.sae.ordonnanceur.support" />

   <!-- traitement de masse -->
   <bean id="traitementMasseLauncher"
      class="fr.urssaf.image.sae.ordonnanceur.support.impl.TraitementMasseLauncherSupportImpl">
      <qualifier value="traitementMasseLauncher" />
      <constructor-arg value="${sae.traitementmasse.executable}" />
      <constructor-arg value="${sae.traitementmasse.executable.xms}" />
      <constructor-arg value="${sae.traitementmasse.executable.xmx}" />
      <constructor-arg ref="saeConfigResource" />
   </bean>

</beans>