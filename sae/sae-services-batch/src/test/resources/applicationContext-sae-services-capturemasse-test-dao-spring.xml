<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
      http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

   <bean id="storageDocumentService" factory-bean="mockFactory"
      factory-method="createStorageDocumentService" primary="true">
      <qualifier value="storageDocumentService" />
   </bean>

   <bean id="provider" factory-bean="mockFactory" factory-method="createStorageServiceProvider"
      primary="true">
      <qualifier value="storageServiceProvider" />
   </bean>

   <bean id="saeDocumentService"
      class="fr.urssaf.image.sae.services.batch.capturemasse.SAEDocumentTestImpl"
      primary="true" />

   <bean id="stepExecutionDao" class="fr.urssaf.image.sae.utils.SaeStepExecutionDao"
      primary="true" />

   <bean id="jobExecutionDao" class="fr.urssaf.image.sae.utils.SaeJobExecutionDao"
      primary="true" />

</beans>
