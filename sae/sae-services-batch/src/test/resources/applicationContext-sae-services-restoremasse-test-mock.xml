<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
   xsi:schemaLocation="
         http://www.springframework.org/schema/beans 
         http://www.springframework.org/schema/beans/spring-beans.xsd
	     http://www.springframework.org/schema/context 
	     http://www.springframework.org/schema/context/spring-context.xsd">

   <bean id="storageDocumentService" factory-bean="mockFactory"
      factory-method="createStorageDocumentService" primary="true">
      <qualifier value="storageDocumentService" />
   </bean>
   
   <bean id="jobQueueService" factory-bean="mockFactory"
      factory-method="createJobQueueService" primary="true">
      <qualifier value="jobQueueService" />
   </bean>
   
   <bean id="sommaireFormatValidationSupport" factory-bean="mockFactory"
      factory-method="createSommaireFormatValidationSupport" primary="true">
      <qualifier value="sommaireFormatValidationSupport" />
   </bean>

</beans>
