<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

   <!-- mock du service de capture en masse-->
   <bean id="captureMasseService"
      class="fr.urssaf.image.sae.services.batch.ServiceMockManager"
      factory-method="createSAECaptureMasseService">
   </bean>
   
   <bean id="suppressionMasseService"
      class="fr.urssaf.image.sae.services.batch.ServiceMockManager"
      factory-method="createSAESuppressionMasseService">
   </bean>
   
   <bean id="restoreMasseService"
      class="fr.urssaf.image.sae.services.batch.ServiceMockManager"
      factory-method="createSAERestoreMasseService">
   </bean>

</beans>
